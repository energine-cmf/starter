{
  "name": "energine/starter",
  "description": "Energine is an XML/XSLT content management framework. This is empty project.",
  "type": "project",
  "license": "MIT",
  "keywords": [
    "xml",
    "xslt",
    "framework",
    "cmf"
  ],
  "homepage": "http://energine.net/",
  "require": {
    "php": ">=5.5.0",
    "ext-dom": "*",
    "ext-gd": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "ext-PDO": "*",
    "ext-pdo_mysql": "*",
    "ext-SimpleXML": "*",
    "ext-session": "*",
    "ext-tidy": "*",
    "ext-xsl": "*",
    "symfony/console": "2.6.*@dev",
    "google/recaptcha": "~1.1",
    "eventviva/php-image-resize": "1.5.*"
  },
  "require-dev": {
    "psr/log": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Energine\\": "core/modules",
      "EnergineSite\\": "site/modules"
    }
  },
  "scripts": {
    "nrgn-setup": "php htdocs/index.php setup",
    "nrgn-linker": "php htdocs/index.php setup linker"
  },
